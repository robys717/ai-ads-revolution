{
  "name": "ai-ads-revolution",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:web": "npm --prefix web install && npm --prefix web run build",
    "start": "NODE_ENV=production node server.cjs",
    "dev": "vite",
    "preview": "vite preview",
    "dev:api": "nodemon --quiet server.cjs",
    "dev:web": "vite",
    "dev:all": "concurrently -k -n API,WEB \"npm:dev:api\" \"npm:dev:web\"",
    "dev:web-only": "vite"
  },
  "dependencies": {
    "express": "^4.21.2",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.552.0",
    "multer": "1.4.5-lts.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}