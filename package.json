{
  "name": "ai-ads-revolution",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:web": "npm --prefix web install && npm --prefix web run build",
    "start": "node server.cjs",
    "dev": "vite",
    "preview": "vite preview",
    "dev:api": "nodemon --quiet server.cjs",
    "dev:web": "vite",
    "dev:all": "concurrently -k -n API,WEB \"npm:dev:api\" \"npm:dev:web\"",
    "dev:web-only": "vite",
    "db:init": "node -r dotenv/config scripts/init_db.js",
    "db:migrate": "node scripts/migrate.cjs"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.24",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}